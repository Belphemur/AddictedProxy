{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore.Hosting.Diagnostics": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Refresh": {
    "SeasonRefresh": "01:00:00",
    "EpisodeRefresh": "01:00:00"
  },
  "Performance" : {
    "SampleRate": 0.75
  },
  "AllowedHosts": "*",
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "RealIpHeader": "X-Real-IP",
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "IpWhitelist": [
      "127.0.0.1"
    ],
    "EndpointWhitelist": [],
    "ClientWhitelist": [],
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "1s",
        "Limit": 15
      },
      {
        "Endpoint": "*",
        "Period": "15m",
        "Limit": 200
      },
      {
        "Endpoint": "*",
        "Period": "12h",
        "Limit": 2000
      }
    ]
  }
}